#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define fori(s,n) for(int i = s; i < n; ++i)
#define forj(s,n) for(int j = s; j < n; ++j)
#define bend(a) a.begin(), a.end()
#define aback(a,i) a[a.size()-i]

int main() {
    ios::sync_with_stdio(0); cin.tie(0);
    int t; cin >> t;
    while(t--){
        int n; cin >> n;
        vector<int> p(n);
        set<int> s;
        int m = 0;
        fori(0,n){
            int cur; cin >> cur;
            if((cur==1 || cur==m-1 || cur==m+1) && !s.count(m)){ 
                cout << m << " ";
                s.insert(m);
                ++m;
            } else{
                cout << m-cur << " ";
                s.insert(m-cur);
            }
        }
        cout << endl;
    }
}
